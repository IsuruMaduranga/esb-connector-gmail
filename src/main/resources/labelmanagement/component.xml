<component name="labelmanagement" type="synapse/template" displayName="Label Management">
    <subComponents>
        <component name="listLabels">
            <displayName>List Labels</displayName>
            <file>listLabels.xml</file>
            <description>List all labels</description>
        </component>
        <component name="readLabel">
            <displayName>Read Label</displayName>
            <file>readLabel.xml</file>
            <description>Get a label detail</description>
        </component>
        <component name="createLabels">
            <displayName>Create Labels</displayName>
            <file>createLabels.xml</file>
            <description>create new label</description>
        </component>
        <component name="updateLabels">
            <displayName>Update Labels</displayName>
            <file>updateLabels.xml</file>
            <description>Update existing labels</description>
        </component>
        <component name="deleteLabel">
            <displayName>Delete Label</displayName>
            <file>deleteLabel.xml</file>
            <description>Delete labels</description>
        </component>
    </subComponents>
</component>